{
  "name": "@biothings-explorer/bte-trapi",
  "version": "2.8.1",
  "description": "BioThings Explorer's TRAPI Knowledgebase Query API",
  "main": "src/server.js",
  "scripts": {
    "setup": "npm run clone && (npm i || true) && npm i",
    "clone": "./scripts/clone_packages.sh",
    "pull": "./scripts/pull_packages.sh",
    "git": "./scripts/run_git_packages.sh",
    "compile": "tsc -b tsconfig.build.json",
    "prepare": "npm run compile",
    "build": "npm run compile",
    "get_rev": "./scripts/get_rev.sh",
    "watch": "tsc -b tsconfig.build.json --watch",
    "watch2": "nodemon -e js,mjs,json,ts --watch packages --exec npm run compile",
    "start": "./scripts/start_server.sh",
    "stop": "./scripts/stop_server.sh",
    "basic-start": "nodemon --ignore './data/' src/server.js",
    "debug": "DEBUG=biomedical-id-resolver,bte* npm run basic-start",
    "debug-all": "DEBUG=* nodemon --ignore './data/' src/server.js",
    "clean": "rimraf \"packages/**/built\" \"packages/**/*.tsbuildinfo\"",
    "clean_on_prod": "rimraf \"packages/**/__test__\" \"packages/**/.git\" \"packages/**/node_modules\"",
    "prettier": "prettier \"*.{js,json,yml,md}\" \"packages/**/*\"",
    "format": "npm run prettier -- --write",
    "format:check": "npm run prettier -- --check",
    "lint": "npm run format:check",
    "test": "jest --forceExit --env node",
    "test-all": "jest --",
    "test-cov": "jest --coverage --env node",
    "release": "standard-version",
    "release:minor": "standard-version --release-as minor",
    "release:patch": "standard-version --release-as patch",
    "release:major": "standard-version --release-as major",
    "siege-local": "artillery run -e local --output report_local.json --config performance-test/config.yaml performance-test/scenarios/query.yaml",
    "smartapi_sync": "DEBUG=bte* SYNC_AND_EXIT=true node ./scripts/smartapi_sync.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/biothings/biothings_explorer.git"
  },
  "keywords": [
    "bte",
    "biothings",
    "single",
    "hop",
    "query"
  ],
  "author": "BioThings Team",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/biothings/biothings_explorer/issues"
  },
  "homepage": "https://github.com/biothings/biothings_explorer#readme",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.29.3",
    "@typescript-eslint/parser": "^4.29.3",
    "artillery": "^1.6.2",
    "cookie-parser": "~1.4.4",
    "coveralls": "^3.1.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.1",
    "http-errors": "~1.6.3",
    "jest": "^29.5.0",
    "lerna": "^6.6.1",
    "morgan": "~1.9.1",
    "nodemon": "^2.0.20",
    "prettier": "^2.3.1",
    "pug": "^3.0.2",
    "rimraf": "^3.0.2",
    "standard-version": "^9.1.1",
    "supertest": "^6.1.3",
    "supports-color": "^7.2.0",
    "typescript": "4.3.4"
  },
  "dependencies": {
    "@biothings-explorer/query_graph_handler": "^1.18.0",
    "@biothings-explorer/smartapi-kg": "^4.0.0",
    "@bull-board/express": "^5.0.0",
    "axios": "^0.21.2",
    "body-parser": "^1.19.0",
    "bull": "^4.10.4",
    "compression": "^1.7.4",
    "connect-history-api-fallback": "^2.0.0",
    "cors": "^2.8.5",
    "debug": "^4.3.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.3",
    "express-rate-limit": "^5.2.6",
    "express-winston": "^4.1.0",
    "helmet": "^4.4.1",
    "husky": "^4.3.8",
    "ioredis": "^5.0.6",
    "js-yaml": "^4.1.0",
    "lodash": "^4.17.21",
    "lz4": "^0.6.5",
    "nanoid": "^3.1.31",
    "node-cron": "^2.0.3",
    "npm": "^9.4.1",
    "openapi-validator-middleware": "^3.2.2",
    "piscina": "^3.2.0",
    "ps-node": "^0.1.6",
    "snake-case": "^3.0.4",
    "stream-chunker": "^1.2.8",
    "stream-json": "^1.7.3",
    "valid-url": "^1.0.9",
    "winston": "^3.3.3",
    "winston-daily-rotate-file": "^4.5.1"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "workspaces": [
    "./packages/@biothings-explorer/api-response-transform",
    "./packages/@biothings-explorer/call-apis",
    "./packages/@biothings-explorer/query_graph_handler",
    "./packages/@biothings-explorer/smartapi-kg",
    "./packages/biolink-model",
    "./packages/biomedical_id_resolver",
    "./src/web-app",
    ".",
    "./packages/@biothings-explorer/node-expansion"
  ]
}
